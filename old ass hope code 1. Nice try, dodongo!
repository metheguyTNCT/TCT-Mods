/* BASIC STRUCTURE */
input[type="radio"] {
  appearance: none;
  -webkit-appearance: none;
  width: 15px;
  height: 15px;
  background: #f4f4f4;
  border: 2px outset #ccc;
  margin: 0 6px 0 2px;
  display: inline-block;
  vertical-align: middle;
  box-shadow:
    inset 2px 2px 0 #fff,
    inset -2px -2px 0 #888;
  position: relative;
  cursor: pointer;
}

/* DOT (MOCK PHYSICAL BUTTON FEEL) */
input[type="radio"]:checked::before {
  content: "";
  position: absolute;
  top: 3px;
  left: 3px;
  width: 7px;
  height: 7px;
  background: black;
  border-radius: 0;
  box-shadow:
    inset 0 0 1px #000;
}

/* "CLICKED IN" FEEL */
input[type="radio"]:active {
  border: 2px inset #999;
  box-shadow:
    inset 1px 1px 0 #bbb,
    inset -1px -1px 0 #666;
}

input[type="radio"]:checked {
  transform: translateY(1px);
}
input[type="radio"]:hover {
  box-shadow:
    0 0 0 1px #aaa,
    inset 1px 1px 0 #fff,
    inset -1px -1px 0 #888;
}
