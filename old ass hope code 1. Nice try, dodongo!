playPauseButton.addEventListener("click", function () {
  const audio = document.getElementById("audio");
  if (audio.paused) {
    audio.play();
    tinyPlay.style.visibility = "visible";
    tinyPause.style.visibility = "hidden";
  } else {
    audio.pause();
    tinyPause.style.visibility = "visible";
    tinyPlay.style.visibility = "hidden";
  }
});

