let crtOn = true;
function toggleCRT() {
    crtOn = !crtOn; // Flip ON/OFF
    if (crtOn) {
        campaignTrail_temp.candidate_image_url = "https://files.catbox.moe/vpk7bp.gif";
        campaignTrail_temp.running_mate_image_url = "https://files.catbox.moe/fcrql6.gif";
    } else {
        campaignTrail_temp.candidate_image_url = "https://files.catbox.moe/caux3s.png";
        campaignTrail_temp.running_mate_image_url = "https://files.catbox.moe/8l930o.png";
    }
    updateCandidateImages(); // <- You might need to refresh the display if required.
}
var setCRTLines = (enabled) => {
  crtStyle.innerText = enabled ? "" : ".crt::before {background: none;}";
}
footer = $(".footer")[0]
