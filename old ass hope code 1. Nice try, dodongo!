function updateTimeDisplay(audio) {
  const currentTimeElement = document.getElementById("current-time");
  const totalTimeElement = document.getElementById("total-time");

  // Convert currentTime to mm:ss format
  const currentMinutes = Math.floor(audio.currentTime / 60);
  const currentSeconds = Math.floor(audio.currentTime % 60);
  currentTimeElement.textContent = `${currentMinutes}:${currentSeconds < 10 ? "0" : ""}${currentSeconds}`;

  // Total time (fixed) based on the song's duration
  const currentSong = playlist.getCurrentSong();
  totalTimeElement.textContent = currentSong.getDuration();
}




  const audio = document.createElement("audio");
  audio.id = "audio";
  audio.src = playlist.getCurrentSong().audioLink;
  audio.volume = 1/9;


audio.addEventListener("ended", function () {
    // Play the next song when the current song ends
    playlist.playNext();
    updateUI(playlist);
    audio.src = playlist.getCurrentSong().audioLink;
    audio.play();

    // Reset the time display when a new song starts
    updateTimeDisplay(audio);
});

audio.addEventListener("timeupdate", function () {
    updateTimeDisplay(audio);
});

      let currentVolume = 6; // Initial volume
      const volume1Get = document.getElementById("volume1");
        const volume2Get = document.getElementById("volume2");
        const volume3Get = document.getElementById("volume3");

  
      volumeUpButton.addEventListener("click", function () {
        const audio = document.getElementById("audio");
        if (currentVolume < 20) { // Increased max volume range
            currentVolume++;
            audio.volume = currentVolume / 20; // Adjusted for new range
            updateVolumeIndicators(); // Call to update visibility
        }
    });
    
    volumeDownButton.addEventListener("click", function () {
        const audio = document.getElementById("audio");
        if (currentVolume > 0) {
            currentVolume--;
            audio.volume = currentVolume / 20; // Adjusted for new range
            updateVolumeIndicators(); // Call to update visibility
        }
    });
  
  function updateVolumeIndicators() {
    // Update visibility based on currentVolume
    updateVolume1();
    updateVolume2();
    updateVolume3();
}
  
    function updateVolume1() {
        if (currentVolume > 0) {
          volume1.style.visibility = "visible";
        }
        else {
          volume1.style.visibility = "hidden";
        }
    }

        function updateVolume2() {
        if (currentVolume > 4) {
          volume2.style.visibility = "visible";
        }
        else {
          volume2.style.visibility = "hidden";
        }
    }

            function updateVolume3 () {
        if (currentVolume > 8) {
          volume3.style.visibility = "visible";
        }
        else {
          volume3.style.visibility = "hidden";
        }
    }
  
     function updateAudioVolume() {
      const audio = document.getElementById("audio");
      audio.volume = currentVolume / 9; // Set volume between 0 and 1
    }

  document.body.appendChild(audio);
  audio.play();

  const abButton = document.createElement("div");
  abButton.id = "ab";
  abButton.addEventListener("click", () => {
  usingSecondary = !usingSecondary;

  if (usingSecondary) {
    changePlaylist(secondaryPlaylist);
  } else {
    changePlaylist(primaryPlaylist);
  }
});

abButton.addEventListener("mousedown", () => {
  abButton.style.transform = "translateY(1px)";
});
abButton.addEventListener("mouseup", () => {
  abButton.style.transform = "translateY(0px)";
});
abButton.textContent = "A/B";

playerContainer.appendChild(abButton);

