const grainVideo = document.createElement('video');
Object.assign(grainVideo, {
  src:      'https://files.catbox.moe/yby0oi.mp4',
  autoplay: true,
  loop:     true,
  muted:    true,
});
Object.assign(grainVideo.style, {
  position:        'absolute',
  top:             '0',
  left:            '0',
  width:           '100%',
  height:          '100%',
  objectFit:       'cover',
  opacity:         '0.051',            // dial this down until it’s as subtle as you like
  pointerEvents:   'none',
  zIndex:          '9998',
  mixBlendMode:    'screen',          // ← add this
  filter:          'blur(3px)'
});

function updateGrainOverlay() {
  if (document.body.classList.contains('crt')) {
    if (!document.body.contains(grainVideo)) {
      document.body.appendChild(grainVideo);
      grainVideo.currentTime = 0;   // rewind to start
      grainVideo.play();            // restart playback
    }
  } else {
    if (document.body.contains(grainVideo)) {
      grainVideo.pause();           // stop it cleanly
      document.body.removeChild(grainVideo);
    }
  }
}

// add it now if CRT is already on
updateGrainOverlay();

// and hook into your toggle
// (assuming toggleCRT() is defined just below)
const originalToggle = toggleCRT;
toggleCRT = function(){
  originalToggle();
  updateGrainOverlay();
};
