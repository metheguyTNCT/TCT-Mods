// Tooltips


function addDynamicCSS() {
  var css = `

      .mytooltip {
          position: relative;
          display: inline-block;
          cursor: pointer;
          text-decoration: underline;
          color: blue;
      }
     .mytooltip .mytooltiptext::before {
      content: "Tooltip - Microsoft Internet Explorer"; /* The text to display */
      top: -20px;
      left: 0px;
      outline: solid 2px #C0C0C0; 
      text-shadow: none;
      text-align: left;
      position: absolute;
      padding-right: 35px;
      padding-left: 4px;
      color: white;
      background: linear-gradient(90deg,navy,#1084d0); /* Match the background to "cut out" the border */
      font-weight: bold;
    }
      .mytooltip:hover {
          color: purple;
      }

      .mytooltip .mytooltiptext {
         max-width: 240px;
          background: linear-gradient(to bottom, #004F6E 0%, #005B82 50%, #006D96 100%);
          color: #d8fffd;
          text-shadow: 1px 1px 2px #313b44;
          text-align: center;
          padding: 10px;
          position: absolute;
          z-index: 99;
          bottom: 20%;
          left: 20%;
          margin-left: -90px;
          opacity: 0;
          overflow: visible;
          transition: opacity 0.3s;
          border-top: 20px solid #000;
          border-left: 1px inset #000;
          border-right: 1px inset #000;
          border-bottom: 1px inset #000;
          outline: outset 3px #C0C0C0;  
          pointer-events: none;
          font-size: 13px !important;
          line-height: 1.5em !important;
          font-weight: normal !important;
          font-style: normal !important;
      }

      .mytooltip .mytooltiptext img {
          max-width: 240px;
          height: auto;
      }

      .mytooltip:hover .mytooltiptext {
          opacity: 1;
          transition-delay: 0.5s;
          box-shadow: inset 0 0 3px rgba(90, 120, 255, 0.25), 0 0 6px rgba(60, 110, 255, 0.15);
          transition-delay: 0.6s;
          @keyframes crtFlicker {
          0%   { opacity: 1; }
          5%   { opacity: 0.9; }
          10%  { opacity: 1; }
          15%  { opacity: 0.95; }
          100% { opacity: 1; }}
}
      }
  `;

  var styleElem = document.createElement('style');
  styleElem.type = 'text/css';
  
  styleElem.appendChild(document.createTextNode(css));
  
  document.head.appendChild(styleElem);
}


addDynamicCSS();

